{
  "name": "@jackinhuang/moa",
  "version": "1.0.1",
  "description": "Open API",
  "keywords": [
    "API Server"
  ],
  "main": "bin/start.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean:dist": "lb-clean dist",
    "clean": "lb-clean dist && lb-clean logs && lb-clean *.pid && lb-clean cache",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "echo \"no test specified.\"",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node $NODE_DEBUG_OPTION .",
    "prepublishOnly": "npm run test",
    "run": "node $NODE_DEBUG_OPTION app.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.0.6",
    "@loopback/boot": "^1.0.3",
    "@loopback/context": "^1.0.1",
    "@loopback/core": "^1.0.1",
    "@loopback/openapi-v3": "^1.1.0",
    "@loopback/repository": "^1.0.3",
    "@loopback/rest": "^1.2.0",
    "@loopback/service-proxy": "^1.0.1",
    "@types/mongodb": "^3.1.22",
    "change-case": "^3.0.2",
    "cross-spawn": "^6.0.5",
    "download-file": "^0.1.5",
    "hashcode": "^1.0.3",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "log4js": "^3.0.6",
    "loopback-connector-mongodb": "^3.9.2",
    "make-dir": "^1.3.0",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^5.1.0",
    "mongodb": "3.1.13",
    "mongodb-schema": "^8.2.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prompts": "^2.1.0",
    "request": "^2.88.0",
    "scan-fs": "0.0.5",
    "through2": "^3.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.0.1",
    "@loopback/testlab": "^1.0.1",
    "@types/node": "^10.11.2",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.1",
    "query-string": "^6.2.0"
  }
}
